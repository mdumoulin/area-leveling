    "repositories": [

    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.7.*",
        "humbug/humbug": "~1.0@dev",
        "phing/phing": "^2.10",
        "halleck45/phpmetrics": "^1.1",
        "phpmd/phpmd": "^2.2",
